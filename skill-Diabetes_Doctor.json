    {
      "type": "response_condition",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "Some diabetes drugs drive blood sugar too low and cause hypoglycemia. This may cause: heart palpitations, anxiety ,hunger ,sweating ,irritability ,fatigue.             Unresolved hypoglycemia may lead to serious complications such as:  clumsiness as if you’re intoxicated ,confusion ,seizures ,loss of consciousness"
              }
            ],
            "response_type": "text",
            "selection_policy": "sequential"
          }
        ]
      },
