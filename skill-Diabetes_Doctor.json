    {
      "type": "response_condition",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "Body Mass Index (BMI) is a person’s weight in kilograms divided by the square of height in meters. A high BMI can indicate high body fatness. BMI screens for weight categories that may lead to health problems, but it does not diagnose the body fatness or health of an individual"
              },
              {
                "text": "A BMI of less than 18.5 means that a person is underweight.\nA BMI of between 18.5 and 24.9 is ideal.\nA BMI of between 25 and 29.9 is overweight.\nA BMI over 30 indicates obesity."
              }
            ],
            "response_type": "text",
            "selection_policy": "sequential"
          }
        ]
      },
      "parent": "node_1_1628410753799",
      "conditions": "@Body_mass_index:(Body mass index)",
